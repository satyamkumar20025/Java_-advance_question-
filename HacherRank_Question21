import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        String S = scan.next();          // reads the input string
        int i   = scan.nextInt();        // start index
        int j   = scan.nextInt();        // end index (exclusive)
        scan.close();

        // substring(beginIndex, endIndex) takes chars [beginIndex .. endIndex-1]
        System.out.println(S.substring(i, j));
    }
}
